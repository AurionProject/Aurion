<wsdl:definitions 
   xmlns:tns="urn:gov:hhs:fha:nhinc:entitynotificationconsumersecured"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
   xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
   xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/"
   xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype" 
   xmlns:nccommonentity="urn:gov:hhs:fha:nhinc:common:nhinccommonentity"
   name="EntityNotificationConsumerSecured" 
   targetNamespace="urn:gov:hhs:fha:nhinc:entitynotificationconsumersecured"
   xmlns:nccommon="urn:gov:hhs:fha:nhinc:common:nhinccommon"
   xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
   xmlns:wspp="http://java.sun.com/xml/ns/wsit/policy"
   xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
   xmlns:wsaw="http://www.w3.org/2005/08/addressing"
   xmlns:ihe="urn:ihe:iti:xds-b:2007" 
   xmlns:sxnmp="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/NMProperty"
   xmlns:vprop="http://docs.oasis-open.org/wsbpel/2.0/varprop"
   xmlns:sp="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy"
   xmlns:sc="http://schemas.sun.com/2006/03/wss/server" xmlns:wsp1="http://www.w3.org/ns/ws-policy" xmlns:sc1="http://schemas.sun.com/2006/03/wss/client">


    <wsdl:types>
        <xsd:schema>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommonentity"
                  schemaLocation="../schemas/nhinc/common/NhincCommonEntity.xsd"/>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommon" schemaLocation="../schemas/nhinc/common/NhincCommon.xsd"/>
        </xsd:schema>
    </wsdl:types>

    <wsdl:message name="NotifySubscribersOfDocumentRequestMessage">
        <wsdl:part name="NotifySubscribersOfDocumentRequest" element="nccommonentity:NotifySubscribersOfDocumentRequest"/>
    </wsdl:message>
    <wsdl:message name="NotifySubscribersOfDocumentResponseMessage">
        <wsdl:part name="NotifySubscribersOfDocumentResponse" element="nccommon:Acknowledgement"/>
    </wsdl:message>
    <wsdl:message name="NotifyRequest">
        <wsdl:part name="NotifyRequest" element="nccommonentity:NotifyRequest"/>
    </wsdl:message>
    <wsdl:message name="NotifyResponse">
        <wsdl:part name="Acknowledgement" element="nccommon:Acknowledgement"/>
    </wsdl:message>
    <wsdl:portType name="EntityNotificationConsumerSecuredPortType">
        <wsdl:operation name="NotifySubscribersOfDocument">
            <wsdl:input name="NotifySubscribersOfDocumentRequest" message="tns:NotifySubscribersOfDocumentRequestMessage" />
            <wsdl:output name="NotifySubscribersOfDocumentResponse" message="tns:NotifySubscribersOfDocumentResponseMessage" />
        </wsdl:operation>

        <wsdl:operation name="Notify">
            <wsdl:input name="NotifyRequest" message="tns:NotifyRequest"/>
            <wsdl:output name="NotifyResponse" message="tns:NotifyResponse"/>
        </wsdl:operation>
    </wsdl:portType>

    <wsdl:binding name="EntityNotificationConsumerSecuredBindingSoap"
                type="tns:EntityNotificationConsumerSecuredPortType">
        <wsp1:PolicyReference URI="#EntityNotificationConsumerSecuredBindingSoapPolicy"/>
        <soap:binding style="document"
                  transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="NotifySubscribersOfDocument">
            <soap:operation soapAction="urn:NotifySubscribersOfDocument"/>
            <wsdl:input name="NotifySubscribersOfDocumentRequest">
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output name="NotifySubscribersOfDocumentResponse">
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
        <wsdl:operation name="Notify">
            <soap:operation soapAction="urn:Notify"/>
            <wsdl:input name="NotifyRequest">
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output name="NotifyResponse">
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>

    <wsdl:service name="EntityNotificationConsumerSecured">
        <wsdl:port name="EntityNotificationConsumerSecuredPortSoap"
               binding="tns:EntityNotificationConsumerSecuredBindingSoap">
            <soap:address
         location=
            "https://localhost:${HttpsDefaultPort}/NhinConnect/EntityNotificationConsumerSecured"/>
        </wsdl:port>
    </wsdl:service>

    <plnk:partnerLinkType name="EntityNotificationConsumerSecured">
    <!-- A partner link type is automatically generated when a new port type is added.
         Partner link types are used by BPEL processes. In a BPEL process, a partner
         link represents the interaction between the BPEL process and a partner service.
         Each partner link is associated with a partner link type. A partner link type
         characterizes the conversational relationship between two services. The
         partner link type can have one or two roles.-->
        <plnk:role name="EntityNotificationConsumerSecuredPortTypeRole"
               portType="tns:EntityNotificationConsumerSecuredPortType"/>
    </plnk:partnerLinkType>
    <wsp1:Policy wsu:Id="EntityNotificationConsumerSecuredBindingSoapPolicy">
        <wsp1:ExactlyOne>
            <wsp1:All>
                <sc1:KeyStore wspp:visibility="private"
                            aliasSelector="gov.hhs.fha.nhinc.callback.KeyStoreClientAliasSelector"
                            callbackHandler="gov.hhs.fha.nhinc.callback.KeyStoreCallbackHandler"/>
                <sc1:TrustStore wspp:visibility="private"
                        certSelector = "gov.hhs.fha.nhinc.callback.TrustStoreAliasSelector"
                        callbackHandler="gov.hhs.fha.nhinc.callback.TrustStoreCallbackHandler"/>

                <sc1:CallbackHandlerConfiguration wspp:visibility="private">
                  <sc1:CallbackHandler name="samlHandler" classname="gov.hhs.fha.nhinc.callback.SamlCallbackHandler"/>
                </sc1:CallbackHandlerConfiguration>
            </wsp1:All>
        </wsp1:ExactlyOne>
    </wsp1:Policy>
</wsdl:definitions>
